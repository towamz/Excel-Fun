Private Sub UserForm_Initialize()
    Dim today As Date
    today = Now()
    
    TxtYrMo.Text = Format(DateSerial(Year(today), Month(today), 1), "yyyymm")
    Call changeCaption(Me, TxtYrMo.Text)

    LblPrevYrMo.Visible = False
End Sub

Private Sub TxtYrMo_Exit(ByVal Cancel As MSForms.ReturnBoolean)
    Call changeCaption(Me, TxtYrMo.Text)
End Sub

Private Sub BtnPrevYr_Click()
    TxtYrMo.Text = getOffsetYearMonthStr(TxtYrMo.Text, -1, 0)
    Call changeCaption(Me, TxtYrMo.Text)
End Sub

Private Sub BtnPrevMo_Click()
    TxtYrMo.Text = getOffsetYearMonthStr(TxtYrMo.Text, 0, -1)
    Call changeCaption(Me, TxtYrMo.Text)
End Sub

Private Sub BtnNextYr_Click()
    TxtYrMo.Text = getOffsetYearMonthStr(TxtYrMo.Text, 1, 0)
    Call changeCaption(Me, TxtYrMo.Text)
End Sub

Private Sub BtnNextMo_Click()
    TxtYrMo.Text = getOffsetYearMonthStr(TxtYrMo.Text, 0, 1)
    Call changeCaption(Me, TxtYrMo.Text)
End Sub


Private Sub btn1_Click()
    Call setDate(LblPrevYrMo.Caption & Format(btn1.Caption, "00"))
    Unload DatePicker
End Sub
